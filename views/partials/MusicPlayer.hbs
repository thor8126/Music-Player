<div class="custom_footer">
    <div class="logo_name">
        <img class="" src="/img/logo.png" alt="Logo" />
        <h2 class="name">Song Name</h2>
        <a href=""><i class="fas fa-heart"></i></a>
    </div>
    <div class="music_btns">
        <div class="top">
            <button class="" id="prev"><i class="fas fa-backward"></i></button>
            <button class="" id="play"><i class="fas fa-play"></i></button>
            <button class="" id="next"><i class="fas fa-forward"></i></button>
        </div>
        <div class="music_slider">
            <span class="current_time">0:00</span>
            <input class="" type="range" id="progress" min="0" max="100" step="1" value="0" />
        </div>
    </div>
    <div class="volume">
        <button class="mute" id="mute"><i class="fas fa-volume-mute"></i></button>
        <input type="range" id="volume" min="0" max="1" step="0.1" value="1" />
    </div>
</div>




